{
  "name": "wigfrid",
  "version": "1.0.0",
  "description": "Wigfrid Loading page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "compress:css": "node-sass css/style.comp.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass compress:css"
  },
  "devDependencies": {
    "autoprefixer": "^9.2.1",
    "concat": "^1.0.3",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1",
    "postcss-prefixer": "^2.1.0",
    "prefix": "^1.0.0",
    "prefixer": "0.0.3"
  },
  "dependencies": {
    "compile": "0.0.2",
    "live-server": "^1.2.0",
    "postcss-prefix-selector": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pale-cinder/Wigfrid.git"
  },
  "author": "pale-cinder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pale-cinder/Wigfrid/issues"
  },
  "homepage": "https://github.com/pale-cinder/Wigfrid#readme"
}
